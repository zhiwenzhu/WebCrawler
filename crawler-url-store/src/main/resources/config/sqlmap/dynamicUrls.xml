<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.zhiwen.crawler.url.store.dao.DynamicUrlsDao">
    <insert id="addUrls" parameterType="com.zhiwen.crawler.url.store.model.DynamicUrls">
        INSERT into ${dbName}
        (
        Url,
        ParentUrl
        )
        VALUES
        (
        #{url},
        #{parentUrl}
        )
    </insert>
</mapper>